{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/pagamento/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/product/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/cart",
      "destination": "/index.html"
    }
  ]
}
